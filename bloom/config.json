{
    "bpm": 89,
    "view": {
        "width": 960,
        "height": 640,
        "target_fps": 60,
        "dynamic": true,
        "vsync": true,
        "screenshot": false,
        "screenshot_path": "output/",
        "locked_speed": false,
        "fullscreen": false
    },
    "server": {
        "ip": "127.0.0.1",
        "port": 3000,
        "enable": false
    },
    "inputs": {
        "InputImage": {
            "type": "Video",
            "path": "res/water.mp4",
            "width": 960,
            "height": 640,
            "speed": {
                "Fps": 50
            }
        }
    },
    "render_chain": [
        {
            "name": "Threshold",
            "filter": "threshold",
            "filter_mode_params": {
                "Rectangle": [
                    0.0,
                    0.0,
                    1.0,
                    1.0
                ]
            },
            "inputs": {
                "iChannel0": {
                    "Linear": "InputImage"
                }
            },
            "variables": {},
            "precision": "U8"
        },
        {
            "name": "Blur1",
            "filter": "blur",
            "filter_mode_params": {
                "Rectangle": [
                    0.0,
                    0.0,
                    1.0,
                    1.0
                ]
            },
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Threshold"
                }
            },
            "variables": {
                "dither": {
                    "Bool": true
                }
            },
            "precision": "U8"
        },
        {
            "name": "Blur2",
            "filter": "blur",
            "filter_mode_params": {
                "Rectangle": [
                    0.0,
                    0.0,
                    1.0,
                    1.0
                ]
            },
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Blur1"
                }
            },
            "variables": {
                "dither": {
                    "Bool": true
                }
            },
            "precision": "U8"
        },
        {
            "name": "Blur3",
            "filter": "blur",
            "filter_mode_params": {
                "Rectangle": [
                    0.0,
                    0.0,
                    1.0,
                    1.0
                ]
            },
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Blur2"
                }
            },
            "variables": {
                "dither": {
                    "Bool": true
                }
            },
            "precision": "U8"
        },
        {
            "name": "Compose",
            "filter": "final_stage",
            "filter_mode_params": {
                "Rectangle": [
                    0.0,
                    0.0,
                    1.0,
                    1.0
                ]
            },
            "inputs": {
                "iChannel0": {
                    "Linear": "InputImage"
                },
                "iChannel1": {
                    "Linear": "Blur3"
                }
            },
            "variables": {},
            "precision": "U8",
            "mipmapped": false
        }
    ],
    "final_stage": {
        "name": "Target",
        "filter": "copy_input",
        "filter_mode_params": {
            "Rectangle": [
                0.0,
                0.0,
                1.0,
                1.0
            ]
        },
        "inputs": {
            "iChannel0": {
                "Linear": "Compose"
            }
        },
        "variables": {},
        "precision": "U8"
    }
}