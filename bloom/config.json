{
    "bpm": 89,
    "view": {
        "width": 640,
        "height": 480,
        "target_fps": 60,
        "dynamic": true,
        "vsync": true,
        "screenshot": false,
        "screenshot_path": "output/",
        "locked_speed": false,
        "fullscreen": false
    },
    "server": {
        "ip": "127.0.0.1",
        "port": 3000,
        "enable": false
    },
    "inputs": {
        "InputImage": {
            "type": "Video",
            "path": "res/butterfly.mp4",
            "width": 1366,
            "height": 768,
            "speed": {
                "Fps": 23.98
            }
        }
    },
    "render_chain": [
        {
            "name": "Threshold",
            "filter": "threshold",
            "inputs": {
                "iChannel0": {
                    "Linear": "InputImage"
                }
            },
            "variables": {},
            "precision": "U8"
        },
        {
            "name": "Blur1",
            "filter": "blur",
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Threshold"
                }
            },
            "variables": {
                "dither": {
                    "Bool": true
                }
            },
            "precision": "U8"
        },
        {
            "name": "Blur2",
            "filter": "blur",
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Blur1"
                }
            },
            "variables": {
                "dither": {
                    "Bool": true
                }
            },
            "precision": "U8"
        },
        {
            "name": "Blur3",
            "filter": "blur",
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Blur2"
                }
            },
            "variables": {
                "dither": {
                    "Bool": true
                }
            },
            "precision": "U8"
        }
    ],
    "final_stage": {
        "name": "Image",
        "filter": "final_stage",
        "inputs": {
            "iChannel0": {
                "Linear": "InputImage"
            },
            "iChannel1": {
                "Linear": "Blur3"
            }
        },
        "variables": {},
        "precision": "U8",
        "mipmapped": false
    }
}