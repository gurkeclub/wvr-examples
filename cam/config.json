{
    "bpm": 120,
    "view": {
        "width": 960,
        "height": 640,
        "fullscreen": false,
        "target_fps": 60,
        "dynamic": true,
        "vsync": true,
        "screenshot_path": "output/",
        "screenshot": false,
        "locked_speed": false
    },
    "server": {
        "ip": "127.0.0.1",
        "port": 3000,
        "enable": false
    },
    "inputs": {
        "Camera": {
            "type": "Cam",
            "path": "/dev/video0",
            "width": 640,
            "height": 480
        }
    },
    "render_chain": [
        {
            "name": "Effect",
            "filter": "dotprint",
            "filter_mode_params": {
                "Rectangle": [
                    0.0,
                    0.0,
                    1.0,
                    1.0
                ]
            },
            "inputs": {
                "iChannel0": {
                    "Mipmaps": "Camera"
                }
            },
            "variables": {},
            "precision": "U8"
        }
    ],
    "final_stage": {
        "name": "Target",
        "filter": "copy_input",
        "filter_mode_params": {
            "Rectangle": [
                0.0,
                0.0,
                1.0,
                1.0
            ]
        },
        "inputs": {
            "iChannel0": {
                "Linear": "Effect"
            }
        },
        "variables": {},
        "precision": "U8"
    }
}